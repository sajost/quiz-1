{# src/AppBundle/Resources/views/Admin/carlogtage.html.twig #} 
{% extends 'admin/init.html.twig' %} 

{% block body %}
	<div id="" class="container">

		<div id="panel" class="panel panel-default">
			<div class="panel-heading">
            		<a class="btn btn-link" href="{{ path('admin_question_tag') }}" style="">Back to Tags</a>
            </div>
			<div class="panel-body">
               <form action="{{ path('admin_question_tag_ne',{'eid':questiontag.id}) }}" method="post" id="f-admin-question-tag" data-toggle="validator" role="form">
                	
                	<div class="form-group">
        				<label for="{{ form.title.vars.id }}">Tag</label>
